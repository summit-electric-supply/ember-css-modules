{
  "name": "@summit-electric-supply/ember-css-modules",
  "version": "1.3.4",
  "description": "CSS Modules for ambitious applications",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:node": "qunit tests-node/**/*-test.js",
    "test:all": "ember try:each"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "bugs": {
    "url": "https://github.com/salsify/ember-css-modules/issues"
  },
  "homepage": "https://github.com/salsify/ember-css-modules#readme",
  "devDependencies": {
    "@ember-decorators/component": "^6.1.1",
    "@ember/optional-features": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-plugin": "^4.0.3",
    "console-ui": "^3.1.2",
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli": "~3.24.0",
    "ember-cli-addon-tests": "^0.11.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-test-loader": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-compatibility-helpers": "^1.2.2",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^5.0.0",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.19.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "postcss-color-rebeccapurple": "^6.0.0",
    "qunit-dom": "^1.6.0",
    "qunitjs": "^2.4.1",
    "sinon": "^9.2.4",
    "template-stuff": "link:tests/dummy/lib/template-stuff",
    "webpack": "^4.46.0"
  },
  "keywords": [
    "ember-addon",
    "css-modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salsify/ember-css-modules.git"
  },
  "license": "MIT",
  "author": "Dan Freeman",
  "dependencies": {
    "@summit-electric-supply/broccoli-css-modules": "^1.0.0",
    "broccoli-bridge": "^1.0.0",
    "broccoli-concat": "^4.2.4",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^6.0.0",
    "calculate-cache-key-for-tree": "^2.0.0",
    "debug": "^4.3.2",
    "ember-cli-babel": "^7.23.1",
    "ember-cli-htmlbars": "^5.3.2",
    "ember-cli-version-checker": "^5.1.2",
    "ensure-posix-path": "^1.1.1",
    "hash-string": "^1.0.0",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.2.5",
    "semver": "^7.3.4",
    "toposort": "^2.0.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-font-awesome"
    ],
    "before": [
      "ember-cli-babel",
      "ember-cli-htmlbars",
      "ember-cli-less",
      "ember-cli-sass",
      "ember-cli-stylus",
      "ember-cli-postcss"
    ]
  },
  "volta": {
    "node": "14.15.5",
    "yarn": "1.22.10"
  },
  "release-it": {
    "git": {
      "changelog": "npx auto-changelog --stdout --template https://gist.githubusercontent.com/atsjj/de6998d6750a8b32778fa3380da24301/raw/66a23990c3324f03aa756e45fed70d46389c7eab/compact.hbs",
      "requireCleanWorkingDir": true,
      "requireUpstream": true,
      "requireCommits": false,
      "addUntrackedFiles": false,
      "commit": true,
      "commitMessage": "${version}",
      "commitArgs": "--gpg-sign",
      "tag": true,
      "tagName": "${version}",
      "tagAnnotation": "${version}",
      "tagArgs": "--sign",
      "push": true,
      "pushArgs": "--follow-tags",
      "pushRepo": "origin"
    },
    "npm": {
      "publish": true
    }
  }
}
